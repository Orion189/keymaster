    "eslint-plugin-react-refresh": "^0.4.24",
    "prettier": "^3.3.3",
    "sass": "^1.94.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^5.4.21"
  }
