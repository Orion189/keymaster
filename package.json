    "@mui/icons-material": "^6.1.4",
    "@mui/material": "^6.1.4",
    "@mui/x-charts": "^7.21.0",
    "@popperjs/core": "^2.11.8",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "electron-squirrel-startup": "^1.0.1",
    "i18next": "^23.16.1",
    "mobx": "^6.13.5",
    "mobx-persist-store": "^1.1.5",
    "mobx-react-lite": "^4.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.3",
    "react-router-dom": "^6.27.0",
    "update-electron-app": "^3.0.0"
