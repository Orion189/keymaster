{
  "name": "keymaster",
  "author": "Yevhen Lepekha",
  "description": "Easy tool to train your typing skill",
  "productName": "KMaster",
  "repository": "https://github.com/Orion189/keymaster",
  "version": "1.0.0",
  "main": "index.mjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier 'src/**/*.{tsx,ts}' --write",
    "preview": "vite preview",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make --arch=arm64",
    "make-mas": "electron-forge make --platform=mas --arch=x64",
    "publish": "electron-forge publish"
  },
  "dependencies": {
    "@electron/universal": "^3.0.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
